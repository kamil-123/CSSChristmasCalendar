<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
{#        <link href='https://fonts.googleapis.com/css?family=PT+Sans:400,700,400italic' rel='stylesheet' type='text/css'>#}
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Lato:ital,wght@0,400;0,700;1,400&display=swap" rel="stylesheet">
        <link href="https://fonts.googleapis.com/css2?family=Fredoka+One&display=swap" rel="stylesheet">
        <link href='https://fonts.googleapis.com/css2?family=Bebas+Neue&display=swap' rel='stylesheet'>
        <!-- Global site tag (gtag.js) - Google Analytics -->
        <script async src="https://www.googletagmanager.com/gtag/js?id=UA-153625913-1"></script>
        <script>
            window.dataLayer = window.dataLayer || [];
            function gtag(){dataLayer.push(arguments);}
            gtag('js', new Date());
            gtag('config', 'UA-153625913-1');
        </script>
        <meta name="viewport" content="width=device-width, initial-scale=1">
	    {% block openGraph %}
            <meta property="og:url" content="https://www.freelify.cz">
	        <meta property="og:type" content="website">
	        <meta property="og:title" content="Freelify.cz - Vyhledávač odborných událostí, konferencí a školení">
	        <meta property="og:description" content="Freelify.cz - Vyhledávač odborných událostí, konferencí a školení.">
	        <meta property="og:image" content="https://www.freelify.cz/images/logo.png">
        {% endblock %}
        <link rel="icon" href="{{ asset('images/favicon.ico') }}" type="image/x-icon">
        {% block stylesheets %}
            <link rel="stylesheet" href="https://bootswatch.com/4/cyborg/bootstrap.min.css">
            {{ encore_entry_link_tags('app-two') }}
        {% endblock %}
        {% block jstop %}{% endblock %}
    </head>
    <body>
        {% include 'inc/homeNavBarTop.html.twig' %}
        {% include 'inc/homenavbar.html.twig' with {'mainHeader': block('mainHeader')}%}
        {% block body %}{% endblock %}
        {{ include('inc/homefooter.html.twig') }}
        {% block javascripts %}
            <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
            <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
            <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
            {{ encore_entry_script_tags('app-two') }}
        {% endblock %}
    </body>
</html>